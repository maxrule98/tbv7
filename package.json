{
	"name": "agenai-trader",
	"private": true,
	"version": "1.0.0",
	"packageManager": "pnpm@8.15.6",
	"scripts": {
		"build": "pnpm -r run build",
		"start:worker": "pnpm --filter @agenai/trader-cli run dev",
		"dev": "pnpm --filter './apps/*' run dev",
		"trader": "pnpm --filter @agenai/trader-cli run dev",
		"build:server": "pnpm --filter @agenai/trader-server... run build",
		"start:server": "pnpm --filter @agenai/trader-server run start",
		"lint": "pnpm -r run lint",
		"test": "pnpm -r run test"
	},
	"devDependencies": {
		"@types/node": "^20.11.30",
		"@typescript-eslint/eslint-plugin": "^7.17.0",
		"@typescript-eslint/parser": "^7.17.0",
		"eslint": "^8.56.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5",
		"vitest": "^1.5.0"
	},
	"dependencies": {
		"protobufjs": "^7.5.4"
	}
}
